apiVersion: v1
kind: Service
metadata:
  name: <%=micro_nombre%>-service
  namespace: <%=micro_namespace%>
  labels:
    app: <%=micro_nombre%>
spec:
  type: ClusterIP
  selector:
    app: <%=micro_nombre%>
  ports:
    - protocol: TCP
      port: 8080
      targetPort: 8080